

var Post = require("../models/posts");
module.exports.create = function(req,res){
    var post = new Post(req.body);
    post.save(function(err,result){
        res.json(result);
    });

}

module.exports.get=function(req,res){
    Post.find({},function(err,result){
        res.json(result);
    })

}
